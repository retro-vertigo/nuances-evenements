<!doctype html>
<html {{ site.language_attributes }}>
<head>
    <meta charset="{{ site.charset }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {{ wp_head }}

    <link rel="icon" href="{{ favicon_link }}favicon.svg" type="image/svg+xml">
    <link rel="icon" href="{{ favicon_link }}favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="{{ favicon_link }}favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="apple-touch-icon" href="{{ favicon_link }}apple-touch-icon.png">
    <link rel="manifest" href="{{ favicon_link }}site.webmanifest">

    {% if acf_options.theme_color %}
        <meta name="theme-color" content="{{ acf_options.theme_color }}">
    {% endif %}

    {# Google Tag set in Cookie Yes plugin #}
    {# {% if acf_options.code_ga %}
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id={{ acf_options.code_ga }}"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', '{{ acf_options.code_ga }}');
        </script>
    {% endif %} #}